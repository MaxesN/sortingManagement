{
    "plugins": ["prettier"],
    "extends": [
        "react-app",
        "react-app/jest"
      ],
      "rules": {
          "no-console": ["warn"],
          "prettier/prettier": [
            "warn",
            {
              "tabWidth": 2,
              "singleQuote": true,
              "semi": false,
              "trailingComma": "es5",
            },
          ],
      }
}